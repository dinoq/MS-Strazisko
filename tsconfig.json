{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "sourceMap": true
    // "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "next-env-custom.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "foto copy.text",
    "src/components/admin/formComponents/FileChooser/FileChooser.tsx", 
    "src/components/header/Header.tsx"  
],
  "exclude": [
    "node_modules"
  ]
}
